           CUSTOMERSALE QUESTIONS
		   
SELECT customerts.CustomerID,customerts.CustFirstName,customerts.CustCity,orders.OrderDate,products.ProductName,categories.CategoryDescription FROM customerts INNER JOIN orders ON customerts.CustomerID=orders.CustomerID INNER JOIN order_details ON orders.OrderNumber = order_details.ordernumber INNER JOIN products ON order_details.ProductNumber=products.ProductNumber INNER JOIN categories ON products.CategoryID=categories.CategoryID WHERE customerts.CustFirstName='MOHAMMAD' && customerts.CustCity='NALGONDA'
1) SELECT customerts.CustomerID,orders.OrderNumber,orders.OrderDate FROM customerts INNER JOIN orders ON customerts.CustomerID=orders.CustomerID 
2) SELECT customerts.CustomerID,customerts.CustFirstName,customerts.CustLastName,orders.OrderNumber,employees.Employee_ID,employees.EmpFirstName,employees.EmpLastName FROM customerts INNER JOIN orders ON customerts.CustomerID=orders.CustomerID INNER JOIN orders_achives ON orders.OrderNumber=orders_achives.OrderNumber INNER JOIN employees ON orders_achives.Employee_ID=employees.Employee_ID WHERE CustCity='NALGONDA'
2b)SELECT customerts.CustomerID,customerts.CustFirstName,customerts.CustLastName,orders.OrderNumber,employees.Employee_ID,employees.EmpFirstName,employees.EmpLastName FROM customerts INNER JOIN orders ON customerts.CustomerID=orders.CustomerID INNER JOIN orders_achives ON orders.OrderNumber=orders_achives.OrderNumber INNER JOIN employees ON orders_achives.Employee_ID=employees.Employee_ID WHERE CustFirstName='MOHAMMAD'
3) SELECT customerts.CustomerID,orders.OrderNumber,orders.OrderTotal,products.ProductNumber,products.ProductName,products.RetailPrice FROM customerts INNER JOIN orders ON customerts.CustomerID=orders.CustomerID INNER JOIN order_details ON orders.OrderNumber=order_details.OrderNumber INNER JOIN products ON order_details.ProductNumber=products.ProductNumber
4)SELECT products.ProductNumber,products.ProductName,products.RetailPrice,vendors.VendorID,vendors.VendName FROM products INNER JOIN product_vendors ON products.ProductNumber=product_vendors.ProductNumber INNER JOIN vendors ON product_vendors.VendorID=vendors.VendorID
5)SELECT customerts.CustomerID,customerts.CustLastName,employees.Employee_ID,employees.EmpLastName FROM customerts INNER JOIN orders_achives ON customerts.CustomerID=orders_achives.customer_Id INNER JOIN employees ON orders_achives.Employee_ID=employees.Employee_ID WHERE customerts.CustLastName ='MOHAMMAD' OR employees.EmpLastName='ILIYAS'
6)SELECT customerts.CustomerID,customerts.CustFirstName,customerts.CustLastName,customerts.CustCity,employees.Employee_ID,employees.EmpFirstName,employees.EmpLastName FROM customerts INNER JOIN orders_achives ON customerts.CustomerID=orders_achives.customer_Id INNER JOIN employees ON orders_achives.Employee_ID=employees.Employee_ID WHERE customerts.CustCity='nalgonda'


           ENTERTAINMENT

1) SELECT engagements.EngagamentNumber,engagements.StartDate,agents.AgentID,agents.AgtFirstName,agents.AgtLastName FROM engagements INNER JOIN agents ON engagements.AgentID=agents.AgentID

2)SELECT customerts.CustomerID,customerts.CustFirstName,customerts.CustLastName,entertainers.EntertainerID,entertainers.EntStageName,entertainers.EntSSN FROM customerts INNER JOIN engagements ON customerts.CustomerID=engagements.CustomerID INNER JOIN engagements_archive ON engagements.EngagamentNumber=engagements_archive.EngagamentNumber INNER JOIN entertainers ON engagements_archive.EntertainerID=entertainers.EntertainerID
2B)SELECT customerts.CustomerID,customerts.CustFirstName,customerts.CustLastName,entertainers.EntertainerID,entertainers.EntStageName,entertainers.EntSSN FROM customerts INNER JOIN engagements ON customerts.CustomerID=engagements.CustomerID INNER JOIN entertainers ON engagements.EntertainerID=entertainers.EntertainerID

SELECT agents.AgentID,agents.AgtFirstName,agents.AgtLastName,agents.AgtZipCode,entertainers.EntertainerID,entertainers.EntStageName,entertainers.EntSSN,entertainers.EntZipCode FROM agents INNER JOIN engagements ON agents.AgentID=engagements.AgentID INNER JOIN entertainers ON engagements.EntertainerID=entertainers.EntertainerID WHERE agents.AgtZipCode='508001' OR entertainers.EntZipCode='508001'

            SCHOOL 
1)SELECT buildings.BuildingCode,buildings.BuildingName,buildings.NumberofFloors,classrooms.ClassRoomID FROM buildings INNER JOIN classrooms ON buildings.BuildingCode=classrooms.BuildingCode    

2)SELECT classes.ClassID,students.StudentID,StudFirstName,StudLastName FROM classes INNER JOIN student_schedules ON classes.ClassID=student_schedules.ClassID INNER JOIN students ON student_schedules.StudentID=students.StudentID

3) 

                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  


      